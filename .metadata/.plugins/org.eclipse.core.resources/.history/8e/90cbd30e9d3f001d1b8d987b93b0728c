package entities;

public class Person {
	private String name;
	private int age;
	private double height;
	
	public Person(String name, int age, double height) {
		this.name = name;
		this.age = age;
		this.height = height;
	}

	public String getName(int p) {
		return name;
	}

	public void setName(String name, int p) {
		this.name = name;
	}

	public int getAge(int p) {
		return age;
	}

	public void setAge(int age, int p) {
		this.age = age;
	}

	public double getHeight(int p) {
		return height;
	}

	public void setHeight(double height, int p) {
		this.height = height;
	}
	
	
	
	
}
